CREATE DATABASE INSURANCES;
USE INSURANCES;

CREATE TABLE PERSON(
    driver_id VARCHAR(10)primary key,
    name VARCHAR(20),
    address VARCHAR(30)
);

CREATE TABLE CAR(
    reg_no VARCHAR(15)primary key,
    model VARCHAR(20),
    year int
);

CREATE TABLE ACCIDENT(
   report_no int primary key,
   acc_date date,
   location VARCHAR(30)
);


